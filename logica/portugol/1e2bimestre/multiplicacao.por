programa {
  funcao inicio() {
    inteiro A, b, M 
    escreva("Informe os valores a serem multiplicados")
    escreva ("O 1o valor a ser multiplicado: ")
    leia (A)
    escreva ("O 2o valor a ser multiplicado: ")
    leia (b)
    M = A*b
    escreva ("A Multiplicação entre", A, " e ", b," é: ", M)
  }
}