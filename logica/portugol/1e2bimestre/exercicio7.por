programa {
  funcao inicio() {
     real n1
    real n2
    real n3
   escreva ("Digite o primeiro numero: ")
    leia (n1)
    escreva ("Digite o segundo numero: ")
    leia (n2)
    escreva ("Digite o terceiro numero: ")
    leia (n3)
    se ((n1> n2 e (n2>n3))) {escreva(n3, " ",n2 , " ", n1)}
    se ((n3> n2 e (n2>n1))) {escreva(n1, " ", n2, " ", n3)}
  }
  }

